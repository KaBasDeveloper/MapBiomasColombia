var rRegionsPath = commonPath + '/DATOS_AUXILIARES/RASTERS/'+country+'/col_ref_inundable11_noraisg';

var col3Path     = commonPath + '/public/collection3/mapbiomas_raisg_panamazonia_collection3_integration_v2';

